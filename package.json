{
  "name": "intelliassist",
  "displayName": "IntelliAssist",
  "description": "IntelliAssist: Code smarter, chat better.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "intelliassist.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "extension.enableCustomPanel",
        "title": "custompanelon"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "highlight.js": "^11.9.0",
    "marked": "^12.0.0"
  },
  "files": [
    "extension.js",
    "media/*",
    "assets/*"
  ]
}
